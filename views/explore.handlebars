  <div>
        <h1 class="text-center font-italic">Explore</h1>
          <p class="lead">See new posts and content.</p>
    </div> 
<br>
 <div class="row justify-content-center">
    <div class="card" style="width: 55rem;">
        <h5 class="card-header">Start exploring!</h5>
        <div class="card-body">

          <ol class="post-list">
            {{#each posts as |post|}}
            <li>
              {{> post-info post }}
            </li>
            {{/each}}
          </ol>

      </div>
    </div>
  </div>
